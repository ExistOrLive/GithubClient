pipeline{
    agent any

    stages{
        stage('Construct Environment'){
            step{
                echo "start Construct Environment"
                whoami
                pwd
                cd ZLGithubClient
                pwd
            }
        }
        stage('Build'){
            step{
                echo "start Construct Environment"
                whoami
                pwd
                cd ZLGithubClient
                pwd
            }
        }
    }
}